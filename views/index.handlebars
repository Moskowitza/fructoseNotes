 <header class="font" >HelloFructose</header>
  <nav>
    <div class="nav-wrapper">
      <a href="#!" id="font" class="brand-logo">Hello Fructose</a>
      <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
      <ul class="right hide-on-med-and-down">
        <li id="scrape"><a href="#">Scrape</a></li>
        <li><a href="#">Github</a></li>
        <li><a href="http://www.hifructose.com">Hi Fructose</a></li>
      </ul>
    </div>
  </nav>
<div class="container">
    <div class="row">
        {{!-- Show my articles --}}
        <div id="articles" class="col l6">
            {{!-- For every article in the DB --}}
            <table>
                <thead>
                    <tr>
                        <th>Title</th>
                        <th>Picture</th>
                        <th>link</th>
                        <th>action</th>
                        <th>notes</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each articles}}
                    <tr data-id={{this._id}}>
                        <td>
                            {{this.title}}
                        </td>
                        <td>
                            <img src={{this.imgLink}}>
                        </td>
                        <td>
                            <a href="{{this.link}}">Link</a>
                        </td>
                        <td>
                            <button class="addNote" data-id="{{this.id}}">Add Note</button>
                            <button class="delete" data-id="{{this.id}}">Remove</button>
                        </td>
                        
                        <td id={{this.note._id}}>
                            {{#each notes}}
                            a)
                            {{this.notes.title}}
                            b)
                            {{note.title}}
                            {{/each}}
                            {{!-- this.note.ObjectId returns nothing --}}
                            {{this.note.ObjectId}} 
                            {{this.note.title}}
                    
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
        <div id="notes" class="col l6">
            note
        </div>
    </div>
</div>